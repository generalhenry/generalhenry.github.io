         <RulesElement name="Wizard" type="Class" internal-id="ID_FMP_CLASS_9" url="http://www.wizards.com/dndinsider/compendium/class.aspx?id=9" charelem="6" legality="rules-legal" >
            <specific name="Key Abilities" > Intelligence; Constitution, Dexterity, or Wisdom </specific>
            <specific name="Implements" > Orbs, staffs, wands, tomes </specific>
            <specific name="Armor Proficiencies" > Cloth </specific>
            <specific name="Weapon Proficiencies" > Dagger, quarterstaff </specific>

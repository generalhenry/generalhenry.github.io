            <RulesElement name="Alarm" type="Ritual" internal-id="ID_FMP_RITUAL_357" url="http://www.wizards.com/dndinsider/compendium/ritual.aspx?id=357" charelem="96" legality="rules-legal" >
               <specific name="Category" > Warding </specific>
